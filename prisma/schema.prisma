// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TaggedFile {
  tagKey       String
  tagValue     String
  fileFullName String

  @@id([tagKey, tagValue, fileFullName])
}

model PresignedDownloadUrls {
  fullname   String   @id
  expiration DateTime
  url        String
}

model AllowedUsers {
  email String @id
}
